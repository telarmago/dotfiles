[user]
    name = telarmago
    email = telarmago@users.noreply.github.com
[alias]
    lg = log --graph --pretty=format:'%C(auto)%h %d %s %C(green)(%cr) %C(blue)<%an>' --abbrev-commit
    s = status
    d = diff
    dc = diff --cached
    b = branch
    co = checkout
    clb = "!f() { git fetch -p && git branch -vv | awk '/: gone]/{print $1}' | xargs git branch -d; }; f"
[core]
    autocrlf = input
